layout( matrix(1:9, 3, 3, byrow = T))

helisy <- read.csv("AminoAcidZPreference_L.csv")
hist(helisy$, xlim = c(-20,20), freq = F, main = "A", xlab = "Z (A)")
lines(density(helisy$A), col = "red")

